  "scripts": {
    "prebuild": "tsx --no-deprecation scripts/bundle-templates.ts",
    "predev": "tsx --no-deprecation scripts/bundle-templates.ts && pnpm run build:agentdock-core",
    "dev": "next dev",
    "build": "pnpm run build:agentdock-core && next build",
    "postbuild": "tsx --no-deprecation scripts/generate-search-index.ts && node scripts/generate-favicons.js && tsx --no-deprecation scripts/generate-llms-from-docs.ts",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "tsx tests/validation/pre-deploy.ts",
    "predeploy": "pnpm run validate",
    "shadcn": "shadcn",
    "generate-favicons": "node scripts/generate-favicons.js",
    "generate-search-index": "tsx --no-deprecation scripts/generate-search-index.ts",
    "git-hooks-validate": "tsx scripts/git-hooks-validate.ts",
    "prepare": "husky",
    "clean-install": "./scripts/clean-install.sh",
    "postinstall": "cd agentdock-core && pnpm install && cd .. && tsx --no-deprecation scripts/generate-llms-from-docs.ts",
    "generate-llms": "tsx --no-deprecation scripts/generate-llms-from-docs.ts",
    "build:agentdock-core": "pnpm --filter agentdock-core build",
    "run-eval-example": "tsx examples/run_evaluation_example.ts"
  },
